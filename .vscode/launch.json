  "version": "0.2.0",
  "configurations": [
    {
      "name": "Lesson 6 Launch",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/lesson_6/code6",
      "args": [],
      "stopAtEntry": true,
      "cwd": "${workspaceFolder}/lesson_6",
      "environment": [],
      "externalConsole": true,
      "MIMode": "gdb",
      "setupCommands": [
          {
              "description": "Enable pretty-printing for gdb",
              "text": "-enable-pretty-printing",
              "ignoreFailures": true
          }
      ]
    },
    {
      "name": "Lesson 6 Docker Launch",
      "type": "cppdbg",
      "request": "launch",
      "program": "code6",
      "args": [],
      "stopAtEntry": false,
      "cwd": "/src/lesson_6",
      "environment": [],
      "externalConsole": true,
      "pipeTransport": {
        "debuggerPath": "/usr/bin/gdb",
        "pipeProgram": "docker-compose",
        "pipeArgs": ["run", "--rm", "lesson6", "sh", "-c"],
        "pipeCwd": ""
      },
      "MIMode": "gdb",
      "setupCommands": [
          {
              "description": "Enable pretty-printing for gdb",
              "text": "-enable-pretty-printing",
              "ignoreFailures": true
          }
      ],
      "sourceFileMap": {
        "/src":"${workspaceFolder}"
      }
    },
    {
      "name": "Lesson 7 Attach",
      "type": "cppdbg",
      "request": "attach",
      "program": "code7",
      "processId": "${command:pickRemoteProcess}",
      "pipeTransport": {
        "debuggerPath": "/usr/bin/gdb",
        "pipeProgram": "docker-compose",
        "pipeArgs": ["exec", "-T", "lesson7", "sh", "-c"],
        "pipeCwd": ""
      },
      "MIMode": "gdb",
      "setupCommands": [
          {
              "description": "Enable pretty-printing for gdb",
              "text": "-enable-pretty-printing",
              "ignoreFailures": true
          }
      ],
      "sourceFileMap": {
        "/src":"${workspaceFolder}"
      }
    },
    {
      "name": "Python: Attach",
      "type": "python",
      "request": "attach",
      "port": 5678,
      "host": "localhost",
      "pathMappings": [
        {
          "localRoot": "${workspaceFolder}",
          "remoteRoot": "/src"
        }
      ]
    },
    {
      "name": "Python: Launch Lesson 4",
      "type": "python",
      "request": "attach",
      "port": 5678,
      "host": "localhost",
      "pathMappings": [
        {
          "localRoot": "${workspaceFolder}",
          "remoteRoot": "/src"
        }
      ],
      "preLaunchTask": "start-lesson4",
      "postDebugTask": "stop-lesson4"
    },
    {
      "name": "Python: Inject Attach",
      "type": "python",
      "request": "attach",
      "port": 5678,
      "host": "localhost",
      "pathMappings": [
        {
          "localRoot": "${workspaceFolder}",
          "remoteRoot": "/src"
        }
      ],
      "preLaunchTask": "attach-lesson4",
    },
